<?xml version="1.0"?>
<Container version="2">
  <Name>QuickChart</Name>
  <Repository>ianw/quickchart</Repository>
  <Registry>https://hub.docker.com/r/ianw/quickchart</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/typpo/quickchart</Project>
  <Overview>QuickChart is a service that generates images of charts from a URL.</Overview>
  <Category>Cloud: Network:Web</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/JmzTaylor/unraid_templates/master/quickchart.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled/>
  <DonateText/>
  <DonateLink/>
  <Description>QuickChart is a service that generates images of charts from a URL. Because these charts are simple images, they are very easy to embed in non-dynamic environments such as email, SMS, chat rooms, and so on.</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data/>
  <Environment/>
  <Labels/>
  <Config Name="Host Port 1" Target="3400" Default="8080" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">8080</Config>
</Container>
